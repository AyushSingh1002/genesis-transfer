# Firebase App Distribution Configuration
# Copy this file to .env and fill in your actual values

# Firebase App ID (get from Firebase Console → Project Settings → Your Android App)
FIREBASE_APP_ID=1:123456789:android:abcdef1234567890abcdef

# Default testers (comma-separated emails)
DEFAULT_TESTERS=tester1@example.com,tester2@example.com

# Default release notes
DEFAULT_RELEASE_NOTES=New build available for testing

# Firebase Project ID
FIREBASE_PROJECT_ID=your-firebase-project-id
